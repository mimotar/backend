{
  "compilerOptions": {
     "target": "ES2023",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    // "typeRoots": ["./node_modules/@types", "./@types"],
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}