
model Profile {
     id                 Int             @id @default(autoincrement())
     address            String? 
     phone_no           String?
     avatar             String?
     bio                String?         @db.VarChar(100)
     verification_no    String?
     verification_type  String?
     next_kin           String?
     next_email         String?
     next_no            String?
     user               User        @relation(fields:[user_id], references:[id])
     user_id            Int         @unique
}

